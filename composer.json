{
  "name": "miaoxing/wechat-qrcode",
  "description": "Miaoxing Wechat Qrcode Plugin",
  "keywords": [
    "miaoxing", "wechat", "qrcode"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "dev-master@dev",
    "miaoxing/wechat": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatQrcode\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatQrcode\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
